								<children>
									<DesignActionConditionTrue id="2157">
										<children>
											<DesignActionUpdateNode id="2158" updAt="$PERSISTENT/Root/Score" resOf="$PERSISTENT/Root/Score - 4"/>
											<DesignActionUpdateNode id="2182" updAt="$PERSISTENT/Root/belowTarget" resOf="$PERSISTENT/Root/belowTarget + 1"/>
