										<children>
											<DesignActionUpdateNode id="2119" updAt="$PERSISTENT/Root/PartList/row[$i]/inventory" resOf="switch ( $i )
 case 3 return &apos;300&apos;
 case 5 return&apos;200&apos;
 case 6 return &apos;750&apos;
 default return &apos;50&apos;"/>
											<DesignActionUpdateNode id="2563" updAt="$PERSISTENT/Root/PartList/row[$i]/received" resOf="$PERSISTENT/Root/PartList/row[$i]/received + $PERSISTENT/Root/PartList/row[$i]/inventory"/>
										</children>
