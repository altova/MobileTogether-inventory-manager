						<children>
							<DesignActionComment id="1949"/>
							<DesignActionComment id="1952" comment="First check if any entry field is empty, if yes, we want to treat it as zero"/>
							<DesignActionCondition id="1847" condition="$PERSISTENT/Root/PartList/row[$i]/order = &apos;&apos;">
								<children>
									<DesignActionConditionTrue id="1848">
										<children>
											<DesignActionUpdateNode id="1849" updAt="$PERSISTENT/Root/PartList/row[$i]/order" resOf="0"/>
										</children>
									</DesignActionConditionTrue>
								</children>
							</DesignActionCondition>
							<DesignActionComment id="1953"/>
