case 1 return CurrBuildKitGAused()
case 2 return CurrBuildKitGZused()
case 3 return CurrBuildKitSPused()
case 4 return CurrBuildKitTJused()
case 5 return CurrBuildKitWGused()
case 6 return CurrConnectorUsed()
default return 0"/>
							<DesignActionUpdateNode id="1760" updAt="$PERSISTENT/Root/PartList/row[$i]/inventory" resOf="switch ($i)
case 1 return CurrBuildKitGAinv()
case 2 return CurrBuildKitGZinv()
case 3 return CurrBuildKitSPinv()
case 4 return CurrBuildKitTJinv()
case 5 return CurrBuildKitWGinv()
