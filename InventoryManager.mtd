					<DesignActionLoop id="2146" varName="i" forEach="(1 to $PERSISTENT/Root/PartNumbersCount )">
						<children>
							<DesignActionCondition id="2147" condition="$PERSISTENT/Root/PartList/row[$i]/inventory_status &gt; 2">
