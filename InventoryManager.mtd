															<DesignActionUpdateNode id="2560" updAt="$PERSISTENT/Root/PartList/row[$i]/inventory" resOf="$PERSISTENT/Root/PartList/row[$i]/received - $PERSISTENT/Root/PartList/row[$i]/used"/>
														</children>
													</DesignActionConditionTrue>
