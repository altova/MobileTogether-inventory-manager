							<DesignActionComment id="1955" comment="If we find any, set them to zero and set the error flag"/>
							<DesignActionCondition id="1817" condition="string-length( $PERSISTENT/Root/PartList/row[$i]/order ) &gt; 0 
and NumberTest($PERSISTENT/Root/PartList/row[$i]/order) = false()">
								<children>
