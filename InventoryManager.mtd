	<XPathFunctionsRoot id="11">
		<children>
			<DesignXPathFunction id="166" xpath="declare function CurrBuildKitSPused()
{
	($ProductSalesMTD/DB/RowSet/Row[1]/@Quantity * 2) + $ProductSalesMTD/DB/RowSet/Row[2]/@Quantity + $ProductSalesMTD/DB/RowSet/Row[3]/@Quantity + ($ProductSalesMTD/DB/RowSet/Row[4]/@Quantity * 2)
}"/>
			<DesignXPathFunction id="167" xpath="declare function CurrBuildKitSPinv()
{
	$PERSISTENT/Root/PartList/row[3]/received - CurrBuildKitSPused()
}"/>
			<DesignXPathFunction id="179" xpath="declare function CurrBuildKitWGused()
{
	 ($ProductSalesMTD/DB/RowSet/Row[2]/@Quantity * 2)  + ($ProductSalesMTD/DB/RowSet/Row[4]/@Quantity * 1) + ($ProductSalesMTD/DB/RowSet/Row[5]/@Quantity * 1)
}"/>
			<DesignXPathFunction id="180" xpath="declare function CurrBuildKitWGinv()
{
	$PERSISTENT/Root/PartList/row[5]/received - CurrBuildKitWGused()
}"/>
			<DesignXPathFunction id="183" xpath="declare function CurrBuildKitGAused()
{
	($ProductSalesMTD/DB/RowSet/Row[1]/@Quantity * 1) 
}"/>
			<DesignXPathFunction id="184" xpath="declare function CurrBuildKitGAinv()
{
	$PERSISTENT/Root/PartList/row[1]/received - CurrBuildKitGAused()
}"/>
			<DesignXPathFunction id="187" xpath="declare function CurrBuildKitGZused()
{
	($ProductSalesMTD/DB/RowSet/Row[2]/@Quantity * 1)  
}"/>
			<DesignXPathFunction id="188" xpath="declare function CurrBuildKitGZinv()
{
	$PERSISTENT/Root/PartList/row[2]/received - CurrBuildKitGZused()
}"/>
			<DesignXPathFunction id="191" xpath="declare function CurrBuildKitTJused()
{
	 ($ProductSalesMTD/DB/RowSet/Row[4]/@Quantity * 1) 
}"/>
			<DesignXPathFunction id="192" xpath="declare function CurrBuildKitTJinv()
{
	$PERSISTENT/Root/PartList/row[4]/received - CurrBuildKitTJused()
}"/>
			<DesignXPathFunction id="195" xpath="declare function CurrConnectorUsed()
{
	($ProductSalesMTD/DB/RowSet/Row[1]/@Quantity * 2) + ($ProductSalesMTD/DB/RowSet/Row[2]/@Quantity * 6) + ($ProductSalesMTD/DB/RowSet/Row[4]/@Quantity * 4) + ($ProductSalesMTD/DB/RowSet/Row[5]/@Quantity * 1)
}"/>
			<DesignXPathFunction id="196" xpath="declare function CurrConnectorInv()
{
  $PERSISTENT/Root/PartList/row[6]/received - CurrConnectorUsed()
}"/>
			<DesignXPathFunction id="973" xpath="declare function IsBlack($a)
{
                count( ( 2, 4, 6 )[ char( $a, . ) &gt;= &apos;0&apos; and char( $a, . ) &lt;= &apos;7&apos; ] ) &gt;= 2
}"/>
			<DesignXPathFunction id="1819" xpath="declare function NumberTest($a) as xs:boolean
{
	string(number($a)) != &apos;NaN&apos;
}"/>
			<DesignXPathFunction id="2082" xpath="declare function TimeAsString() as xs:string
{

if ($PERSISTENT/Root/SimTime &lt; 1200 )
then
concat (
substring (
	 (pad-string-left( $PERSISTENT/Root/SimTime ,  4 ,  &apos;0&apos;))
, 1, 2)
, &apos;:00 AM&apos;)
else
concat (
substring (
	 (pad-string-left( $PERSISTENT/Root/SimTime ,  4 ,  &apos;0&apos;))
, 1, 2)
, &apos;:00 PM&apos;)
}"/>
		</children>
	</XPathFunctionsRoot>
