							<DesignActionUpdateNode id="1800" updAt="$PERSISTENT/Root/PartList/row[$i]/inventory" resOf="$PERSISTENT/Root/PartList/row[$i]/received - $PERSISTENT/Root/PartList/row[$i]/used"/>
