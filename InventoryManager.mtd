			<DesignActionGroup id="710" name="Populate Avg Demand">
				<children>
					<DesignActionLoop id="730" varName="i" forEach="( 1 to $PERSISTENT/Root/PartNumbersCount  )">
						<children>
							<DesignActionUpdateNode id="2384" updAt="$PERSISTENT/Root/tempHold" resOf="switch ( $i )
 case 3 return &apos;300&apos;
 case 5 return&apos;200&apos;
 case 6 return &apos;750&apos;
 default return &apos;50&apos;"/>
							<DesignActionUpdateNode id="731" updAt="$PERSISTENT/Root/PartList/row[$i]/avgdemand" resOf="if ( $PERSISTENT/Root/PartList/row[$i]/used =&apos;&apos; or  $PERSISTENT/Root/PartList/row[$i]/used &lt;  $PERSISTENT/Root/tempHold ) 
  then $PERSISTENT/Root/tempHold 
  else ($PERSISTENT/Root/PartList/row[$i]/used  ) div day-from-date( $PERSISTENT/Root/CurrentDate )"/>
							<DesignActionCondition id="1787" condition="$PERSISTENT/Root/PartList/row[$i]/avgdemand &gt; 0">
								<children>
									<DesignActionConditionTrue id="1788">
										<children>
											<DesignActionUpdateNode id="1785" updAt="$PERSISTENT/Root/PartList/row[$i]/inventory_status" resOf="( $PERSISTENT/Root/PartList/row[$i]/inventory) div ($PERSISTENT/Root/PartList/row[$i]/avgdemand )"/>
										</children>
									</DesignActionConditionTrue>
									<DesignActionConditionFalse id="1789">
										<children>
											<DesignActionUpdateNode id="1786" updAt="$PERSISTENT/Root/PartList/row[$i]/inventory_status" resOf="10"/>
										</children>
									</DesignActionConditionFalse>
								</children>
							</DesignActionCondition>
						</children>
					</DesignActionLoop>
					<DesignActionComment id="2533" comment="Special case where date &lt; 8 and we want to fill avgdemand with forecast"/>
					<DesignActionUpdateNode id="2498" updAt="$PERSISTENT/Root/tempHold" resOf="$PERSISTENT/Root/SimTime"/>
					<DesignActionCondition id="2499" condition="day-from-date( $PERSISTENT/Root/CurrentDate ) &lt; 8">
						<children>
							<DesignActionConditionTrue id="2500">
								<children>
									<DesignActionUpdateNode id="2501" updAt="$PERSISTENT/Root/SimTime" resOf="&apos;2359&apos;"/>
									<DesignActionReloadData id="2536" reloadRef="152"/>
									<DesignActionUpdateNode id="2537" updAt="$PERSISTENT/Root/SimTime" resOf="$PERSISTENT/Root/tempHold"/>
								</children>
							</DesignActionConditionTrue>
						</children>
					</DesignActionCondition>
					<DesignActionComment id="2548" comment="End of special case"/>
					<DesignActionLoop id="2545" varName="i" forEach="( 1 to $PERSISTENT/Root/PartNumbersCount  )">
						<children>
							<DesignActionUpdateNode id="2546" updAt="$PERSISTENT/Root/PartList/row[$i]/avgdemand" resOf="switch ( $i )
 case 1 return CurrBuildKitGAused() div day-from-date( $PERSISTENT/Root/CurrentDate )
 case 2 return CurrBuildKitGZused() div day-from-date( $PERSISTENT/Root/CurrentDate )
 case 3 return CurrBuildKitSPused() div day-from-date( $PERSISTENT/Root/CurrentDate )
 case 4 return CurrBuildKitTJused() div day-from-date( $PERSISTENT/Root/CurrentDate )
 case 5 return CurrBuildKitWGused() div day-from-date( $PERSISTENT/Root/CurrentDate )
 case 6 return CurrConnectorUsed() div day-from-date( $PERSISTENT/Root/CurrentDate )
 default return &apos;100&apos;"/>
							<DesignActionUpdateNode id="2547" updAt="$PERSISTENT/Root/PartList/row[$i]/inventory_status" resOf="( $PERSISTENT/Root/PartList/row[$i]/inventory) div ($PERSISTENT/Root/PartList/row[$i]/avgdemand )"/>
						</children>
					</DesignActionLoop>
					<DesignActionReloadData id="1638" reloadRef="1552"/>
					<DesignActionReloadData id="1637" reloadRef="1561"/>
					<DesignActionReloadData id="1230" reloadRef="1221"/>
				</children>
