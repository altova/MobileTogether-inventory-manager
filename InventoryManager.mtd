	case 1 return &apos;BuildKitGA&apos;
    case 2 return &apos;BuildKitGZ&apos;
    case 3 return &apos;BuildKitSP&apos;
    case 4 return&apos;BuildKitTJ&apos;
    case 5 return &apos;BuildKitWG&apos;
    case 6 return &apos;Connector&apos;
	default return concat(&apos;Unknown00&apos;,string( $i - 6 ))"/>
